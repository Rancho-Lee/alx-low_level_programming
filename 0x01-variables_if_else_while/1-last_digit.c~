#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/* A more complicated conditional structure*/
/**
 * main - returning an integer value for succes
 *
 * Return: 0;
 *
 */
int main(void)
{

		int n;
		int last_digit;


			srand(time(0));

				n = rand() - RAND_MAX / 2;
				last_digit = n % 10;

					/* your code goes there */

					return (0);

}
